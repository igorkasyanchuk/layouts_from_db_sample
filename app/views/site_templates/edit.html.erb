<h1>Editing site_template</h1>

<% form_for(@site_template) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :layout %><br />
    <%= f.text_area :layout %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @site_template %> |
<%= link_to 'Back', site_templates_path %>